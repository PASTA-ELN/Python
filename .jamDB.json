{
  "-userID": "*INITIALS-UUID*",
  "-defaultLocal": "local",
  "-defaultRemote": "remote",
  "-eargs": {
    "editor": "emacs",
    "ext": ".org",
    "style": "all"
  },
  "local": {
    "user": "*USER*",
    "password": "*PASSWORD*",
    "database": "*UUID*",
    "path": "*jamDB-folder*"
  },
  "temporary_test": {
    "user": "*USER*",
    "password": "*PASSWORD*",
    "database": "temporary_test",
    "path": "temporary_test"
  },
  "remote": {
    "user": "*USER-bluemix*",
    "password": "*PASSWORD_2*",
    "url": "https://*USER-bluemix*.cloudantnosqldb.appdomain.cloud",
    "database": "*UUID*"
  }
}